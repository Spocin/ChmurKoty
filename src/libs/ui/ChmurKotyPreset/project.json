{
  "name": "ui-chmur-koty-preset",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src/libs/ui/ChmurKotyPreset/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": ["type_ui"],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "src/libs/ui/ChmurKotyPreset/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "src/libs/ui/ChmurKotyPreset/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "src/libs/ui/ChmurKotyPreset/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "src/libs/ui/ChmurKotyPreset/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
